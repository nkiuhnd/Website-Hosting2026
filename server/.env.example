# Server configuration
PORT=4000

# Database configuration
DATABASE_URL="file:/app/server/prisma/dev_v2.db"

# JWT configuration
JWT_SECRET="replace_with_your_secure_jwt_secret"

# Admin user configuration
ADMIN_USERNAME="admin"
ADMIN_PASSWORD="change_this_password"

# RAM子用户信息（第一步获取）
ALIYUN_ACCESS_KEY_ID="replace_with_your_access_key_id"
ALIYUN_ACCESS_KEY_SECRET="replace_with_your_access_key_secret"

# 短信认证服务信息（第二步获取，如使用）
ALIYUN_SMS_SIGN_NAME="速通互联验证码"
ALIYUN_SMS_TEMPLATE_CODE="100001"

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379

# Site URL configuration
# Local Development: http://localhost:4000
# Production: http://yunmind.cn
PUBLIC_BASE_URL=http://localhost:4000
